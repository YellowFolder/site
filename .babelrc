{
	"env": {
		"development": {
			"presets": ["next/babel"],
			"plugins": [
				"macros",
				"preval",
				[
					"styled-components",
					{
						"ssr": true,
						"displayName": true,
						"preprocess": "false"
					}
				]
			]
		},
		"production": {
			"presets": ["next/babel"],
			"plugins": [
				"macros",
				"preval",
				[
					"styled-components",
					{
						"ssr": true,
						"displayName": true,
						"preprocess": "false"
					}
				]
			]
		},
		"test": {
			"presets": [
				[
					"next/babel",
					{
						"preset-env": {
							"modules": "commonjs"
						}
					}
				]
			],
			"plugins": [
				"macros",
				"preval",
				[
					"styled-components",
					{
						"ssr": true,
						"displayName": true,
						"preprocess": "false"
					}
				]
			]
		}
	}
}
