{
	"name": "yellowfolder.com",
	"version": 2,
	"public": true,
	"routes": [
		{
			"src": "/(.*)",
			"status": 301,
			"headers": { "Location": "https://yellowfoldercom.yellowfolder.now.sh/$1" }
		},
		{
			"src": "/robots.txt",
			"dest": "/static/robots.txt"
		},
		{
			"src": "/logo-horizontal@3x.png",
			"dest": "/static/images/logo-horizontal@3x.png"
		}
	]
}
