{
	"version": 2,
	"name": "site",
	"build": {
		"env": {
			"DOMAIN": "site.brockthebear.now.sh"
		}
	},
	"builds": [{ "src": "package.json", "use": "@now/next" }],
	"routes": [
		{ "src": "/feed.json", "dest": "/_next/static/feed.json" },
		{ "src": "/posts/(?<page>[^/]+)$", "dest": "/posts?page=$page" }
	]
}
