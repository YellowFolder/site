{
	"name": "yellowfolder.com",
	"version": 2,
	"public": false,
	"build": {
		"env": {
			"DOMAIN": "yellowfolder.com"
		}
	},
	"builds": [{ "src": "package.json", "use": "@now/next" }],
	"routes": [
		{
			"src": "/(.*)/",
			"status": 301,
			"headers": { "Location": "/$1" }
		},
		{
			"src": "^(/static/.*)",
			"dest": "$1"
		},
		{
			"src": "/robots.txt",
			"dest": "/static/robots.txt"
		},
		{
			"src": "/logo@3x.png",
			"dest": "/static/images/logo-horizontal@3x.png"
		}
	]
}
