{
	"name": "yellowfolder.com",
	"version": 2,
	"public": false,
	"build": {
		"env": {
			"DOMAIN": "yellowfolder.com"
		}
	},
	"builds": [
		{
			"src": "package.json",
			"use": "@now/next"
		},
		{
			"src": "index.html",
			"use": "@now/static"
		}
	],
	"routes": [
		{
			"src": "(.*)",
			"dest": "$1.html",
			"status": 301,
			"headers": { "Location": "/$1" }
		},
		{
			"src": "^(/static/.*)",
			"dest": "$1"
		},
		{
			"src": "/robots.txt",
			"dest": "/static/robots.txt"
		}
	]
}
