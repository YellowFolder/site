{
	"name": "yellowfolder.com",
	"version": 2,
	"public": true,
	"alias": ["https://www.yellowfolder.com"],
	"build": {
		"env": {
			"DOMAIN": "https://yellowfolder.com"
		}
	},
	"routes": [
		{
			"src": "/(.*)",
			"status": 301,
			"headers": { "Location": "https://yellowfolder.com/$1" }
		},
		{
			"src": "/robots.txt",
			"dest": "/static/robots.txt"
		},
		{
			"src": "/logo-horizontal@3x.png",
			"dest": "/static/images/logo-horizontal@3x.png"
		}
	]
}
