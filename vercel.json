{
	"version": 2,
	"public": false,
	"build": {
		"env": {
			"DOMAIN": "yellowfolder.com",
			"FRESHDESK_KEY_PROD": "@freshdesk-key-prod-production-wddg",
			"NEXT_PUBLIC_FRESHDESK_KEY_PROD": "@next-public-freshdesk-key-prod-production-46mx",
			"NEXT_PUBLIC_FRESHDESK_BASE_URL": "@next-public-freshdesk-base-url-preview-bd3j"
		}
	},
	"routes": [
		{
			"src": "/(.*)/",
			"status": 301,
			"headers": { "Location": "/$1" }
		},
		{
			"src": "^(/static/.*)",
			"dest": "$1"
		},
		{
			"src": "/robots.txt",
			"dest": "/static/robots.txt"
		},
		{
			"src": "/logo@3x.png",
			"dest": "/static/images/logo-horizontal@3x.png"
		}
	]
}
